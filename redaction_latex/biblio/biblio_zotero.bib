@book{mounierReadWriteBook2012,
  title = {Read/Write Book 2 : Une introduction aux humanités numériques},
  shorttitle = {Read/Write Book 2},
  editor = {Mounier, Pierre},
  translator = {family=(CVCE, given=Équipe du Centre, prefix=virtuel de la connaissance sur l’Europe, useprefix=false and Allard, Laurence and Grosdemouge, Pierre and Lamé, Marion and Luxembourg) and Pailler, Fred},
  date = {2012-09-21},
