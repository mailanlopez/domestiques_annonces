@article{bozonSignificationsSocialesActes1999,
  title = {Les significations sociales des actes sexuels},
  author = {Bozon, Michel},
  date = {1999},
  journaltitle = {Actes de la Recherche en Sciences Sociales},
  volume = {128},
  number = {1},
  pages = {3--23},
  publisher = {{Persée - Portail des revues scientifiques en SHS}},
  doi = {10.3406/arss.1999.3288},
  url = {http://www.persee.fr/doc/arss_0335-5322_1999_num_128_1_3288},
  urldate = {2021-06-15},
  abstract = {Der soziale Sinn des Sexualaktes Körperliche Aktivitäten sind in der menschlichen Sexualität nicht eine Frage des Instinkts, sondern das Ergebnis eines sozialen Lernens. Indes wird das Ineinandergreifen von Sexualakten und der durch sie verkörperten Bedeutungsinhalte selten analysiert, zunächst schon infolge mangelnder Sichtbarkeit der physischen Sexualität, aber auch, weil den Betroffenen die Beschreibung ihrer sexuellen Praktiken schwerfällt, und sie dagegen Widerstände entwickeln. In keiner Kultur und zu keiner historischen Epoche ist die sexuelle Aktivität in aller öffentlichkeit praktiziert worden, jedoch hat sich mit fortschreitender Konstruktion der westlichen Innerlichkeit die Verheimlichung der Sexualität in entscheidendem Maße verstärkt. Zwar fällt es leicht, die physische Sexualität in metaphorischer Form anzudeuten, aber selbst in Untersuchungen über sexuelles Verhalten wird sie von den Akteuren nur mit Schwierigkeiten ausgesprochen und benannt. Trotz ihres Privatcharakter war die sexuelle Aktivität immer schon Gegenstand von Beschreibungen, guten Ratschlägen und öffentlichem Gespräch. Während langer Zeit diente das Rubrizieren und Zensieren sexueller Akte als Kontrollinstrument der sie implizierenden sozialen Beziehungen. Als im 19. Jahrhundert die Sexualsphäre zunehmend Autonomie erlangte, bildete sich eine « wissenschaftliche » Sprache ihres Beschreibens heraus, die dem Einzelnen die Möglichkeit eröffnete, sich auf der Grundlage seines Vehaltens als Subjekt zu begreifen, und die von nun an die Psychologisierung desselben begleitete. Wenn die Klassifizierungsprinzipien der sexuellen Akte sich auch geändert haben, so ist die in den zur Beschreibung der Grundakte verwendeten Metaphern ablesbare physische Sexualität doch immer wieder nur durch die geschlechtliche Dominierung stukturiert geblieben. Die gegenwärtige Entwicklung, innerhalb derer in den sexuellen Beziehungen der Begriff des Spiels aufgewertet und demokratisiert wurde, hat im übrigen denjenigen der Transgression als eines gefährlichen Jenseits der sexuellen Normalität nicht zum Verschwinden gebracht. Zweifellos steht die Verbergung des sexuellen Akts letztlich mit der doppelten Tatsache in Berührung, daß er als ein zwar leicht zu beeinträchtigendes und wenig stabiles, aber grundlegend bedeutsames Ritual wahrgenommen wird, weil sich die geschlechtliche Dominierung in ihm auf « magische » Weise materialisiert.},
  langid = {fre},
  file = {/home/mailan/Zotero/storage/AXMKAQKF/arss_0335-5322_1999_num_128_1_3288.html}
}

