@article{kramplPresseAnnoncesParisienne2020,
  title = {La presse d’annonces parisienne à la fin de l’Ancien Régime. Modes de communication et effets sociaux d’un nouveau média},
  author = {Krampl, Ulrike},
  date = {2020},
  journaltitle = {Histoire, économie \& société},
