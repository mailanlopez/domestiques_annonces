@article{noblePassionateFictionsPortraits1997,
  title = {"{{Passionate}} Fictions": Portraits of Female Masculinity in {{The Well}} of {{Loneliness}} and {{Stone Butch Blues}}},
  shorttitle = {"{{Passionate}} Fictions"},
  author = {Noble, Jean},
  date = {1997-24},
  journaltitle = {Resources for Feminist Research},
  volume = {25},
  number = {3/4},
  pages = {92--101},
  publisher = {{Resources for Feminist Research}},
  location = {{Toronto, Canada}},
  issn = {07078412},
  url = {http://www.proquest.com/docview/194913886/abstract/2FB8F11722B148DAPQ/1},
  urldate = {2022-02-10},
  abstract = {Taken individually, The Well of Loneliness and Stone Butch Blues seem as though they are worlds apart. However, when read side by side they seem like the inverted mirror image of each other, sharing stock features, metaphors and tropes, characters and conventions, yet separated by history, geography and class. The Well opens with a "Commentary" by its own discursive "mentor" [Havelock Ellis], while Stone Butch Blues opens with an acknowledgment of the Stonewall "butches, passing women, drag kings and drag queens, FTM [Female to Male] brothers and MTF [Male To Female] sisters -- transsexual and transvestite, "a taxonomy similar to those categories found in sexology only now authorized by the signature "Stonewall" ([Leslie Feinberg], p. 5). The Well also opens with a genealogy of [Stephen Gordon]'s family name and place; Stone Butch Blues opens with an account of two working class and "already bitterly disappointed people," [Jess Goldberg]'s parents (Feinberg, p. 13). Later, Stephen roams the pastoral family estate at Malvern Hills; Jess roams the unpaved streets of the "projects" in Buffalo, New York. Stephen identifies with the portrait of "young [Nelson]" as well as "the funny old portraits of the Gordons" while Jess queers narratives of masculinity such as "Captain Midnight," "The Fugitive," union men on strike, and the leading men of adult movies during the Saturday matinees at the local theatre. In their home, [Mary Llewellyn] attends to the intimate details of Stephen's wardrobe. Likewise, in Stone Butch Blues, butch-femme eroticism is not only signified by the details of the "butches" performance of masculinity, but by the femmes' recognition of those details: "Jackie smoothed my lapels and nodded in approval"; "Peaches fussed with my lapels"; "Edna held my eyes as she walked over to me and smoothed my lapels"; "The femmes just kind of smoothed the lapels of my suit coat and held me in their gaze" (Feinberg, pp. 30, 58, 103, 108). Stephen's body does not fit her gender identification and she is taunted with the question "What is it?" while Jess's also doesn't fit, and is similarly taunted with the same refrain, "How the hell should I know what it is." Stephen characterizes herself as "never...worshipped in return by the creature of [her] adoration" while Jess too is a "stone" butch. Stephen's world evolves around the strength of masculine figures -- Stephen herself, [Philip Gordon], even Stephen's rival for Mary, Martin Hallam -- while Jess's world evolves around figures of femininity -- Theresa, Jacqueline, Angie, Edna and Ruth -- to whom Jess returns, and by whom Jess is tended, after every beating.},
  langid = {english},
  pagetotal = {0},
  keywords = {Eye movements,Families \& family life,Gays \& lesbians,History \& criticism,Literature,Metaphor,Motion pictures,Onomastics,Parents \& parenting,Postmodernism,Rhetorical figures,Siblings,Social classes,Social Sciences: Comprehensive Works,Women's Studies}
}

