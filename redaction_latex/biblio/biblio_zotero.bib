@article{zylberberg-hocquardOuvrieresEtatTabacsAllumettes1978,
  title = {Les Ouvrières d'{{Etat}} ({{Tabacs-Allumettes}}) Dans Les Dernières Années Du {{XIXe}} Siècle},
  author = {Zylberberg-Hocquard, Marie-Hélène},
  date = {1978},
  journaltitle = {Le Mouvement social},
  number = {105},
  eprint = {3777551},
  eprinttype = {jstor},
  pages = {87--107},
  publisher = {{Association Le Mouvement Social}},
  issn = {0027-2671},
  doi = {10.2307/3777551},
  url = {https://www.jstor.org/stable/3777551},
  urldate = {2023-07-13},
  abstract = {Les manufactures de Tabac offrent à l'étude du travail féminin un terrain à la fois privilégié et exceptionnel. Privilégié parce que la documentation (imprimé et archives) est abondante, bien regroupée, presque inexploitée, parce que les mouvements de femmes y ont été particulièrement nombreux et qu'on peut y entendre l'expression d'une parole d'ouvrière. Exceptionnel parce que le statut fait aux ouvrières d'Etat, supérieur à celui des autres ouvrières par le salaire et les avantages sociaux (pas de chômage, stabilité d'emploi, retraite, etc.) contribue à produire un type d'ouvrières minoritaires dans l'ensemble français: ouvrières à temps complet, qui font "carrière", comme les hommes, et déploient une activité collective (grèves, syndicalisme) de haut niveau. Terrain d'expérience technique et de rationalisation du travail (les ingénieurs y sont très actifs), les manufactures d'Etat, en ce tournant du siècle confrontent les femmes à de nouvelles exigences de production et de discipline. Cette dernière est définie par des règlements pointilleux et par l'autorité des "petits chefs" souvent méprisants pour ces "troupeaux" de femmes. Mais celles-ci manifestent une grande aptitude à la résistance quotidienne et parviennent à maintenir une surprenante liberté de geste et de parole. Voilà un milieu féminin très original dont on comprend qu'il ait pu constituer un idéal pour beaucoup d'autres travailleuses en même temps qu'un scandale pour tous ceux qui n'acceptent d'ouvrières que les midinettes. /// Tobacco manufactures are a privileged and exceptional field for the study of women's work Privileged: there is a great wealth of printed and archival sources which are to be found in few places; women's actions were especially numerous; the speech of women workers can be heard. Exceptional: the status given to State women workers implied higher wages and better welfare systems than for other women workers (no unemployment, job safety, pensions, and so on); hence those women were a distinct minority in the French labour force: fulltime women workers with a "career" (like men), using collective action (through unions and strikes) at a high level. The State manufactures were a safe ground for technical innovations and labour rationalization. Thus, at the end of the past century, they confronted the women with the new managerial demands for production and discipline. Discipline was settled by far-fetching regulations and by the authority of foremen who often despised the "herds" of women who obeyed their orders. However those women were quite able to display a very active daily resistance, and they succeeded in maintaining a surprisingly high freedom of speech and gesture. This very original section of the female labour force became indeed a model for many other women workers, and at the same time it was a scandal for those who considered the midinettes to be the only normal women workers.},
  file = {/home/mailan/Zotero/storage/KV58VPKH/Zylberberg-Hocquard_1978_Les ouvrières d'Etat (Tabacs-Allumettes) dans les dernières années du XIXe.pdf}
}
