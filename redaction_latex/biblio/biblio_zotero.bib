@article{tillyStructureEmploiTravail1978,
  title = {Structure de l'emploi, Travail Des Femmes et Changement Démographique Dans Deux Villes Industrielles: {{Anzin}} et {{Roubaix}}, 1872-1906},
  shorttitle = {Structure de l'emploi, Travail Des Femmes et Changement Démographique Dans Deux Villes Industrielles},
  author = {Tilly, Louise A.},
  date = {1978},
  journaltitle = {Le Mouvement social},
  number = {105},
  eprint = {3777549},
  eprinttype = {jstor},
  pages = {33--58},
  publisher = {{Association Le Mouvement Social}},
  issn = {0027-2671},
  doi = {10.2307/3777549},
  url = {https://www.jstor.org/stable/3777549},
  urldate = {2023-07-13},
  abstract = {Quelles ont été, à court terme, les répercussions du travail des femmes sur les structures familiales et sur la fécondité? A long terme, quels rapports y a-t-il eu entre le travail salarié des femmes et le déclin de la fécondité conjugale? En comparant les données disponibles sur Anzin et Roubaix en 1872 à partir des listes nominatives du recensement et en étudiant des individus et des familles, on observe que le niveau de l'emploi féminin n'est pas seul responsable d'une fécondité élevée. Les deux villes ont des structures d'emploi féminin différentes, mais un point commun: les travailleurs sont surtout des célibataires. Les femmes mariées ne travaillent que si elles sont dans le besoin. A Roubaix dans le textile, les familles ouvrières demandaient aux épouses de se comporter en productrices et en reproductrices, du moins jusqu'à ce que leurs enfants les remplacent comme ouvriers. Dans leurs stratégies l'emploi des enfants comptait autant que celui des épouses. A Roubaix comme à Anzin, la main-d'œuvre se caractérisait par sa jeunesse, aussi bien dans le textile que dans les charbonnages. C'est cette spécificité qui explique la similitude des attitudes des familles. Elles avaient beaucoup d'enfants car ceux-ci pouvaient gagner un salaire dès leur jeune âge. La durée relativement courte de la vie de travail des adultes impliquait que les parents aient besoin des enfants survivants pour les aider à l'approche de la vieillesse En 1906, dans les deux villes, le déclin de la fécondité est patent. Il est lié à la fois au déclin de l'industrie et aussi au nombre décroissant d'enfants au travail. Les offres d'emploi pour ce type de main-d'œuvre ont pratiquement disparu. C'est à la maison qu'étaient prises les décisions qui déterminaient quels membres de la famille travailleraient, ainsi que les décisions sur la reproduction. L'entité familiale, avec sa composition propre, ses perspectives d'emploi, ses espérances et sa vision de l'avenir était au cœur du processus de décision. /// How, in the short run, did levels and characteristics of women's labor force participation affect family structure and fertility? In what ways, over the long run, were changes in the level and characteristics of women's wage work linked to declining marital fertility? Comparison of Anzin and Roubaix in 1872 based on analysis of individuals and families from the census nominal lists shows that several factors contributed to high fertility besides levels of female employment. The occupational structures of the industrial towns had different levels of female employment, but they were similar in that it was primarily single women who worked. The wives who worked worked because of need. Textile families in Roubaix called on wives to act as producers and reproducers, at least until their children replaced them as workers. Opportunities for children's work were as important in the textile families' calculations as was the work of wives. Youth was characteristic of the labor force of Roubaix and of Anzin. It was the age structures of occupations, which both the textile and mining industries shared, which led families to act in similar ways. The had many children because children could earn wages when they were young. The relatively short work-life spans of adults meant that surviving children were needed to help their parents as they approached old age. Fertility had declined in both cities by 1906. Both the decline of the industry and decreased numbers of children working were involved. The structure of opportunity in the two cities had changed in similar ways with the disappearance of chances for child labor. The household was the site of decisions about who worked; it was the locus also of decisions about reproduction. The situation of the household as an entity, its composition and its range of employment opportunities, its hopes and expectations for the future were central to such decisions.},
  file = {/home/mailan/Zotero/storage/E3VBRK8U/Tilly_1978_Structure de l'emploi, travail des femmes et changement démographique dans deux.pdf}
}

@book{tissotsylvieGayfriendlyAcceptationControle2018,
  title = {Gayfriendly: acceptation et contrôle de l'homosexualité à Paris et à New York / Sylvie Tissot},
  shorttitle = {Gayfriendly},
  author = {{Tissot Sylvie}},
  date = {2018},
  series = {Cours et travaux},
  publisher = {{Raisons d'agir}},
  location = {{Paris}},
  abstract = {Que veut dire être gayfriendly ? Avoir des amis gais ? Soutenir le « mariage pour tous » ? Envisager sans effroi que sa fille devienne lesbienne ? Sortir dans des bars gais et même renouveler ses propres pratiques sexuelles ? Il n’y a pas de « bonne » gayfriendliness, mais des attitudes différentes, en France et aux États-Unis, variables selon les âges, le sexe et les parcours de vie. L’acceptation de l’homosexualité, qui progresse indéniablement, n’est pas non plus réservée aux plus riches : ces derniers l’ont plutôt intégrée au sein d’une morale de classe qui leur permet de se distinguer des pauvres, des habitants des banlieues ou encore des populations racisées. Interviewer des hétérosexuels de milieu aisé montre que, dans des espaces de tolérance et de mixité comme le Marais à Paris et Park Slope à Brooklyn, le contrôle n’a pas disparu : la sympathie s’exprime avant tout en direction de gays et de lesbiennes de même statut socioéconomique, qui manifestent leur envie de couple et de famille, et mettent en sourdine tout autre revendication. La gayfriendliness a donc fait reculer la violence et les discriminations ; elle accompagne aussi l’invention, par les femmes surtout, de modes de vie moins conventionnels. Pourtant, si elle a mis fin à certains préjugés, elle ne s’est pas encore complètement affranchie de ce qui reste un élément structurant de nos sociétés : la domination hétérosexuelle},
  isbn = {978-2-912107-99-2},
  langid = {fre},
  pagetotal = {321},
  keywords = {CLASSE SOCIALE,DISCRIMINATION,États-Unis,France,HOMOSEXUALITE,Homosexualité,HOMOSEXUALITY,INTEGRATION,LIFE STYLES,MODE DE VIE,New York (N.Y.),NORME SOCIALE,Normes sociales,OPINION PUBLIQUE,Paris (France),Préjugés,PUBLIC OPINION,Quartiers gays et lesbiens,SOCIAL CLASSES,SOCIAL NORMS,Tolérance}
}

