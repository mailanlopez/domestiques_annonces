@thesis{marolleauHomosexualiteFeminineEcran2015,
  title = {L'homosexualité féminine à l'écran: quelle visibilité pour les lesbiennes au cinéma américain et dans les séries télévisées américaines},
  shorttitle = {L'homosexualité féminine à l'écran},
  author = {Marolleau, Emilie},
  date = {2015},
  journaltitle = {L'homosexualité féminine à l'écran : quelle visibilité pour les lesbiennes au cinéma américain et dans les séries télévisées américaines},
  url = {http://www.theses.fr/2015TOUR2006/document},
  urldate = {2022-02-16},
  abstract = {Ce travail de recherche s’attache à analyser la visibilité des lesbiennes au cinéma américain et dans les séries télévisées et les web-séries américaines. Bien qu’ayant été globalement invisibles ou mal représentées dans le cinéma classique hollywoodien, elles apparaissent de plus en plus fréquemment sur le petit et le grand écran. Toutefois, cette visibilité est construite à partir d’un certain nombre de normes, privilégiant donc une image particulière des lesbiennes à travers notamment la figure de la lesbienne féminine. En outre, cette visibilité repose sur des stratégies de réappropriations et d’emprunts qui visent à « queeriser » les représentations existantes, et, possiblement, à créer un regard lesbien The purpose of this work is to investigate lesbian visibility in American cinema, TV series and web series. Although lesbians have remained broadly invisible or misrepresented in classical Hollywood Cinema, they now increasingly appear on both the big screen and the small screen. However, this visibility is shaped according to a precise set of norms, thus favoring a specific image of lesbians, particularly through the figure of the feminine lesbian. Furthermore, this visibility is grounded on strategies of reappropriation and borrowings aiming to queer existing representations, and, possibly, to create a lesbian look},
  langid = {fre},
  keywords = {Cinema,Cinéma,Homosexualité,Homosexuality,Lesbians,Lesbiennes,Queer,Séries télévisées,Television,Télévision,TV series,Visibilité,Visibility,Web series,Web-séries}
}

