@article{ungererValeursUrbainesPropre1986,
  title = {Les Valeurs Urbaines Du Propre: {{Blanchissage}} et Hygiène à {{Paris}} Au {{XVIII}} e Siècle},
  shorttitle = {Les Valeurs Urbaines Du Propre},
  author = {Ungerer, Catherine},
  date = {1986},
  journaltitle = {Ethnologie française},
  volume = {16},
  number = {3},
  eprint = {40988931},
  eprinttype = {jstor},
  pages = {295--298},
  publisher = {{Presses Universitaires de France}},
  issn = {0046-2616},
  url = {https://www.jstor.org/stable/40988931},
  urldate = {2023-09-06},
  abstract = {Au XVIIIe siècle des médecins enrichissent le débat sur l'hygiène vestimentaire d'arguments nouveaux et préconisent en particulier l'entretien très fréquent du linge. Des exemples pris dans la réalité parisienne soulignent le décalage entre le discours médical et les pratiques. Le blanchissage semble handicapé par un équipement insuffisant et insalubre, et par la mauvaise réputation des blanchisseuses, mais le livre de comptes d'un blanchisseur permet d'affiner la perception de cette pratique et de la différencier suivant le sexe et l'âge. La situation parisienne illustre les contradictions entre un urbanisme médiéval qui soutient toujours l'économie et un discours qui tend à devenir dominant. During the eighteenth century doctors contributed to the debate on vestimentary hygiene, bringing new arguments and recommending, in particular, the very frequent washing of linen. Examples taken from the reality of Parisian life underline the distance separating this medical discourse from the actual practices in the city. Laundering seems to have been handicapped by inadequate and insalubrious equipment and by the bad reputation of washerwomen. A launderer's account book enables us to sharpen our perception of practices of clothes washing, and to differentiate them according to sex and age. The situation in Paris illustrates the contradictions between mediaeval town life which still supported the economy and a discourse which was tending to become the dominant one.},
  file = {/home/mailan/Zotero/storage/3ISEBKCZ/Ungerer_1986_Les valeurs urbaines du propre.pdf}
}

@article{vanderheijdenWomenVictimsSexual2000,
  title = {Women as {{Victims}} of {{Sexual}} and {{Domestic Violence}} in {{Seventeenth-Century Holland}}: {{Criminal Cases}} of {{Rape}}, {{Incest}}, and {{Maltreatment}} in {{Rotterdam}} and {{Delft}}},
  shorttitle = {Women as {{Victims}} of {{Sexual}} and {{Domestic Violence}} in {{Seventeenth-Century Holland}}},
  author = {family=Heijden, given=Manon, prefix=van der, useprefix=true},
  date = {2000},
  journaltitle = {Journal of Social History},
  volume = {33},
  number = {3},
  eprint = {3789214},
  eprinttype = {jstor},
  pages = {623--644},
  publisher = {{Oxford University Press}},
  issn = {0022-4529},
  url = {http://www.jstor.org/stable/3789214},
  urldate = {2021-06-27},
  abstract = {This article started with questions about the position of women in criminal cases of rape, incest, and maltreatment in seventeenth-century Holland. Did contemporaries consider women who suffered from rape, incest, and maltreatment as victims of sexual and physical abuse? Furthermore, did honour play any role in these court cases? The purpose of this study was to determine whether raped, assaulted, and maltreated women were viewed as victims by contemporaries. Criminal records show that judges, neighbours, and plaintiffs made significant distinctions among these offenses. According to current views of sexual abuse, raped, assaulted, or maltreated women are without any doubt the victims of their perpetrators. The modern perspective is that victim and perpetrator are prime subjects in such criminal cases. However, seventeenth-century lawyers, judges, plaintiffs, accused, and witnesses did not focus their attention on victim and perpetrator only. Egmond rightly pointed out that judges were also focused on the crime itself. This so-called 'offence-oriented' legal mentality implied that young girls were seen as accomplices or co-offenders in many cases of incest. Furthermore, criminal cases of rape, incest and maltreatment not only concerned the honour and reputation of the victim and the perpetrator, but also of neighbours and family members. Indeed, although the latter did not stand trial or risk conviction, their good reputation was endangered as well.}
}

