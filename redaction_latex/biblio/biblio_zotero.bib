@article{rocheInventionLingeAu1986,
  title = {L'invention Du Linge Au {{XVIII}} e Siècle},
  author = {Roche, Daniel},
  date = {1986},
  journaltitle = {Ethnologie française},
  volume = {16},
  number = {3},
  eprint = {40988924},
  eprinttype = {jstor},
  pages = {227--238},
  publisher = {{Presses Universitaires de France}},
  issn = {0046-2616},
  url = {https://www.jstor.org/stable/40988924},
  urldate = {2023-09-06},
  abstract = {L'argument principal de cet article consiste à démontrer l'importance du linge pour l'histoire de la culture matérielle. Utilisant un millier d'inventaires après décès entre le règne de Louis XIV et celui de Louis XVI, en dépit des biais nombreux de la source, la documentation révèle trois points principaux. Entre la fin du XVIIe siècle et celle du XVIIIe s'instaure à Paris le marché de consommation lingère et non avant. Avant 1700 les indices de diffusion dans les groupes sociaux les plus nombreux sont les plus bas. L'analyse de cette consommation met en valeur, tant pour les hommes que pour les femmes, la hiérarchie sociale et en même temps comment elle se brouille. Enfin, la super fluite l'emporte sur la nécessité qui semble souligner le passage d'un système culturel de consommation où le linge entre à la fois dans la définition des apparences et dans le régime de l'hygiène, à un autre ensemble où le rapport au corps est différent sans totalement l'emporter. The main argument of this article is to show the importance of linen for the history of material culture. It is based on a thousand inventories after death, between the reigns of Louis XIV and Louis XVI. Such a source contains many slants, but it nonetheless reveals three main points. Between the end of the seventeenth century and the end of the eighteenth century Paris witnessed the creation of a market for the consumption of linen. Prior to 1700 there is but little evidence of the diffusion of underlinen amongst most of the sections of the population. For both men and women, the analysis of this consumption serves as an indicator of social hierarchy and also of how this hierarchy is becoming less clear. Finally, during this period, superfluity becomes more important than necessity. This seems to underline the passage — albeit incomplete — from a cultural system of consumption in which linen can be related both to definitions of appearance and to rules of hygiene, to another system in which the relationship to the body is different.},
  file = {/home/mailan/Zotero/storage/ATWMF5KK/Roche_1986_L'invention du linge au XVIII e siècle.pdf}
}

