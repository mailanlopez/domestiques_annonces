@article{rossiaudProstitutionJeunesseSociete1976,
  title = {Prostitution, Jeunesse et Société Dans Les Villes Du {{Sud-Est}} Au {{XVe}} Siècle},
  author = {Rossiaud, Jacques},
  date = {1976},
  journaltitle = {Annales. Histoire, Sciences Sociales},
  volume = {31},
  number = {2},
  eprint = {27580246},
  eprinttype = {jstor},
  pages = {289--325},
  publisher = {{EHESS}},
  issn = {0395-2649},
  url = {http://www.jstor.org/stable/27580246},
  urldate = {2021-06-15},
  abstract = {Prostitution, flourishing and official, sexual violence in the form of collective rapes committed by young men of the town, such were the two apparently contradictory phenomena which affected urban life in south eastern France in the 15th century. The present study is an attempt to explain such behavior by an analysis of the matrimonial structures: middle-aged men, successful rivals of the younger men on the marriage market, provided the latter with municipally controlled fornication (the sexual needs of fellow bachelors were well understood). They accepted and supervised the noisy but socialized functions of the young men's fraternities. Thus, until the beginning of the 16th century, prostitutes were not excluded from society: they acted as mediators for clerics as well as for persons of distinction, bearing witness to humanity's sad condition but helping in the fight against vice and violence.}
}

