@article{bernardInnocenceJuridiqueSuspicion2021,
  title = {De l’innocence juridique à la suspicion judiciaire. Le consentement à l’acte sexuel des fillettes dans les procès pour viol du Châtelet de la seconde moitié du 18e~siècle},
  shorttitle = {De l’innocence juridique à la suspicion judiciaire},
  author = {Bernard, Maëlle},
  date = {2021},
  journaltitle = {Dix-huitième siècle},
  shortjournal = {Dix-huitième siècle},
  volume = {53},
  number = {1},
  pages = {357--373},
  publisher = {{Société Française d'Étude du Dix-Huitième Siècle}},
  issn = {0070-6760},
  doi = {10.3917/dhs.053.0357},
  url = {https://www.cairn.info/revue-dix-huitieme-siecle-2021-1-page-357.htm},
  urldate = {2022-09-27},
  abstract = {LE PEUPLE EN COLÈRE Sous la direction de Sophie Abdela et Pascal Bastien Il semble parfois qu’il n’y ait de mobilisation que populaire. Il semble aussi, parfois, qu’il n’y ait d’action véritable que lorsqu’elle est portée par la colère. Car le «~peuple~», à la différence du «~public~», bouscule. Le deuxième suppose l’unanimité alors que le premier, naturellement canaille, révèle une division. Ce dossier propose d’entendre et de comprendre la colère du peuple~: celle qu’on anticipe, celle qu’on craint et celle qu’on revendique. Les paroles des gens du peuple, sous toutes leurs formes, ont traversé la littérature, l’administration et la pensée politique du 18e siècle et les auteurs des Lumières ont défini, jugé, parfois écouté, souvent inventé, mais toujours réduit, les paroles, les espoirs et les frustrations du populaire. Ce volume entend être attentif à la question du peuple, à celle de la souveraineté, et aux revendications qui lient – parfois avec fracas – l’un à l’autre. GRAND ENTRETIEN «~Carnavalet~: habiter, à nouveau, le 18e siècle~» avec Marie-Laure Deschamps VARIA Les varia s’intéressent aux langues~: dictionnaires du chinois, multilinguisme néerlandais, ou prières en français\,; mais elles sont aussi consacrées aux émotions~: au désir de désobéir, à l’impensable consentement sexuel des fillettes, aux pleurs au spectacle d’Inès de Castro, aux émotions de la sculpture, à l’enfouissement et à l’étalage sadiens. Tous les arts sont honorés avec le parcours du musicien Fay, l’apparition de la figure picturale du jeune dessinateur, les décors muraux de l’hôtel Dangé, l’art du pastel, les héroïnes de Charrière et même la mythographie alchimique de Libois. On lira aussi ce qu’il en est du matérialisme et du communisme deschampsiens, des rapports de la philosophie et de la dérision chez Fontenelle ou de l’art de la paresse du Neveu de Rameau. Enfin, Beccaria fait l’objet d’une série d’articles portant sur la peine de mort, l’arithmétique criminelle, l’