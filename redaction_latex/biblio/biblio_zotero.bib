@article{jaspardViolencesVecuesFantasmes2003,
  title = {Violences Vécues, Fantasmes et Simulacres. {{Comment}} Analyser Les Violences Envers Les Femmes},
  author = {Jaspard, Maryse and Brown, Elizabeth and Condon, Stéphanie and Firdion, Jean-Marie and Fougeyrollas-Schwebel, Dominique and Houel, Annik and Lhomond, Brigitte and Maillochon, Florence and Saurel-Cubizolles, Marie-Josèphe and Schiltz, Marie-Ange},
  date = {2003},
  journaltitle = {Nouvelles Questions Féministes},
  volume = {22},
  number = {3},
  eprint = {40620091},
  eprinttype = {jstor},
  pages = {72--81},
  publisher = {{[Éditions Antipodes, Nouvelles Questions Féministes \& Questions Feministes]}},
  issn = {0248-4951},
  url = {http://www.jstor.org/stable/40620091},
  urldate = {2021-03-04},
  abstract = {L'équipe Enveff (Enquête nationale sur les violences envers les femmes en France) a répondu aux attaques perpétrées par Marcela Iacub et Hervé Le Bras contre sa recherche dans le N°632 des Temps Modernes (2003). Une introduction montrant la portée politique de cette attaque du féminisme précède le texte publié dans le N°633 de la même revue. Le mode le plus courant de ces attaques est la réduction et l'amalgame, le refus de prendre en compte la diversité des positions et les multiples débats à l'intérieur du mouvement et des recherches féministes. L'autre mode, plus fondamental mais tout aussi commun, est la négation du fait que les relations entre les hommes et les femmes sont bien plus que des « interactions individuelles réversibles » , qu'elles constituent des rapports sociaux et que ces rapports sont inégalitaires. Plus de trente ans de travaux sur ces questions, pour ne considérer que la période récente, sont en quelque sorte renvoyés au néant. In a recent volume of Les Temps Modernes, the researchers responsible for the National Survey of Violence Against Women in France respond to criticism of their survey by Marcela Iacub and Hervé Le Bras. In an introduction to volume N°632, they discuss the political implications of this attack on feminism, while the research itself follows in volume N° 633. The most common techniques used in these attacks are over-simplification and stereotyping, and the refusal to take seriously the diversity of positions and the numerous debates within the feminist mouvement and feminist studies. In another more fundamental vein, antifeminist critiques refuse to recognize that relations between women and men are not « interchangeable individual interactions » but rather socially constituted and socially constituted as unequal. In this way, more than thirty years of research on the question of violence against women is deliberately overlooked denigrated.}
}

@thesis{jassiePopularAttitudesAuthority1996,
  title = {Popular Attitudes toward Authority and Justice during the {{Old Regime}} and the {{French Revolution}}: {{Paris}}, 1720-1794},
  shorttitle = {Popular Attitudes toward Authority and Justice during the {{Old Regime}} and the {{French Revolution}}},
  author = {Jassie, Kenneth Neal},
  date = {1996},
  institution = {{ProQuest Dissertations Publishing}},
  url = {https://search.proquest.com/docview/304286012?pq-origsite=primo},
  urldate = {2022-02-21},
  abstract = {From the bread and kidnapping riots of Louis XV's reign to the memorable journees of the Revolution, popular resistance manifested itself as a recurring feature of the eighteenth-century Parisian landscape. Yet, these uprisings involved as much negotiation as violence: insurgents invariably framed their demands in terms of obtaining "justice" and they generally pursued concrete and limited goals. In this dissertation, I elucidate the complex nature of justice enacted "from below," and explore how these popular attitudes informed behavior toward authority. Doing a close reading of particular riots and revolts, I address the following questions: under what circumstances did Parisian artisans and workers rise against their rulers? What forms did the interactions between the popular classes and the authorities take during the Old Regime? Finally, how did the Revolution reinforce as well as transform existing popular expectations and practices? To capture such an intriguing and elusive subject I draw on a variety of documents. I have consulted police reports, newspapers, court records, popular prints, journals, diaries, and letters. A burgeoning historiographical literature on popular culture, society, and unrest has also aided my endeavors. By analyzing and synthesizing the disparate sources on popular attitudes and actions in eighteenth-century Paris, I offer insights into the interdependent relationship between the pre-revolutionary and revolutionary epochs. I argue that the riots and insurrections of the Old Regime and the Revolution originated in the dynamics of the neighborhood communities. Local values regulated the behavior of citizens and monitored the positive and negative impacts of outside authority. When Louis XV and his successor withdrew their paternal bounty, citizens responded by criticizing the monarchy and gradually withdrawing their allegiance. The Revolution provided an alternative political, social, and economic vision, but a largely traditionalist populace overturned the throne only when faced with repeated royal duplicity. Radical journalists and local militants gradually forged the revolutionary message, although the common people adapted the new ideals to their existing beliefs. In the end, laborers and artisans defined liberty, equality, and popular sovereignty on their own terms; they regarded economic and social egalitarianism and direct democracy as their ultimate goals.},
  isbn = {9798684685545},
  langid = {english},
  keywords = {European history}
}

@book{JeTravailleDonc,
