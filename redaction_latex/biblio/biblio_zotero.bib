@article{giuffreOuPlacerLigne2019,
  title = {Où placer la ligne rouge ? La qualification du harcèlement sexuel dans les restaurants},
  shorttitle = {Où placer la ligne rouge ?},
  author = {Giuffre, Patti A. and Williams, Christine L.},
  translator = {Boidin, Carole and Boisson, Hélène},
  date = {2019-09-11},
  journaltitle = {Sociologie du travail},
  volume = {61},
  number = {3},
  publisher = {{Association pour le développement de la sociologie du travail}},
  issn = {0038-0296},
  doi = {10.4000/sdt.21206},
  url = {http://journals.openedition.org/sdt/21206},
  urldate = {2022-08-17},
  abstract = {Comme plusieurs travaux l’ont montré, les salariées sont majoritairement exposées au harcèlement sexuel et souffrent des répercussions négatives qu’il engendre~; pourtant, seule une minorité de ces femmes qualifie ces expériences vécues de «~harcèlement sexuel~». Dans le cadre d’une enquête sur l’identification du harcèlement sexuel, des entretiens approfondis avec dix-huit serveuses et serveurs ont été menés dans des restaurants d’Austin, au Texas. Les personnes interrogées n’ont qualifié certaines avances sexuelles de «~harcèlement sexuel~» que dans quatre contextes spécifiques~: (1) lorsqu’elles sont le fait d’une personne tirant profit de sa position de pouvoir pour obtenir des faveurs sexuelles personnelles~; (2) lorsque l’agression a été commise par une personne d’une race/ethnicité différente de celle de la victime — typiquement, un homme issu d’une minorité harcelant une femme blanche~; (3) lorsque l’agression a été commise par une personne d’une orientation sexuelle différente de celle de la victime — typiquement, un homosexuel harcelant un hétérosexuel~; (4) en cas d’usage de la violence ou de menace de violences. La thèse défendue par les autrices est la suivante~: les normes hégémoniques de ce qui est acceptable en termes d’activité sexuelle favorisent les relations hétérosexuelles, légitiment les formes institutionnalisées d’exploitation sexuelle sur le lieu de travail, et peuvent protéger contre les accusations de harcèlement sexuel les agresseurs appartenant à la même race et ayant la même orientation sexuelle que leurs victimes.},
  issue = {3},
  langid = {french},
  keywords = {Domination,États-Unis,Harcèlement sexuel,Lieu de travail,Normes hégémoniques,Orientation sexuelle,Racisme,Relations de travail,Sexualisation du travail,Sexualité,Texas},
  file = {/home/mailan/Zotero/storage/PP5FGBY7/Giuffre_Williams_2019_Où placer la ligne rouge.pdf}
}

