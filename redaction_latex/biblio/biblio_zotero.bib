@article{martiniRapportsPatrimoniauxCredit1998,
  title = {Rapports patrimoniaux et crédit dans les ménages nobles. Dot et apanage des femmes à Bologne au XIXe siècle},
  author = {Martini, Manuela},
  date = {1998-04-01},
