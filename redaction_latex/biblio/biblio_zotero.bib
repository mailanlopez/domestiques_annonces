@thesis{feyelAnnonceNouvellePresse1994,
  type = {Thèse de 3e cycle},
  title = {L'Annonce et la nouvelle: la presse d'information et son évolution sous l'Ancien Régime},
  shorttitle = {L'Annonce et la nouvelle},
  author = {Feyel, Gilles},
  editora = {Roche, Daniel},
  editoratype = {collaborator},
  date = {1994},
  institution = {{Université Paris 1 Panthéon-Sorbonne}},
  location = {{France}},
  abstract = {[Dans les deux derniers siècles de l'Ancien Régime, s'est progressivement constitué autour de la gazette, un sytème de l'information cohérent. Abandonnant son grand projet du "réglement des pauvres", Renaudot se replie en 1630 sur le "bureau d'adresse", véritable agence d'annonces. Avec la gazette, il rationalise l'information, tout en mettant la nouvelle au service de la propagande de la monarchie. Le "modèle Renaudot" échoue : il n'a pas su établir un "support mixte" juxtaposant la nouvelle et l'annonce. L'annonce exploite alors d'autres médias : les "placards" ou affiches et les "billets" ou tracts. Pendant ce temps, le "double marché de l'information" s'épanouit à travers la gazette et les gazettes "périphériques" imprimés en français à l'étranger, diffusées en France par l'administration des postes. Les années 1750 sont le temps d'une double rupture : l'adoption du modèle allemand des intelligenzblatter permet l'épanouissement de la presse d'annonces, avec les affiches, annonces et avis divers, cependant que la "modération de port" accordée par les postes permet à la gazette, aux gazettes "périphériques" et aux affiches de réduite leurs abonnements et d'être partout répandus. Le Ministère des Affaires étrangères reprend la "Gazette de France" en 1762 et la diffusion des gazettes "périphériques" en 1767. Il contrôle aussi les "journaux politiques" du libraire Panckoucke, ainsi que l'empire de presse de Pierre Benezech : le "bureau de correspondance générale", la gazette d'agriculture, les affiches de Paris et la nouvelle presse provinciale. Ainsi existe-t-il un système de l'information à la fin de l'Ancien Régime, où l'annonce et la nouvelle sont réconciliées sur les affiches, ces "supports mixtes" consacrés à l'utilité et aux lumières.]},
  langid = {french},
  pagetotal = {1632},
  volumes = {5},
  keywords = {Information -- France -- 17e siècle,Information -- France -- 18e siècle,Presse -- France -- 17e siècle,Presse -- France -- 18e siècle},
  file = {/home/mailan/Zotero/storage/NWI28AQR/SRCH.html}
}

